<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 7.0.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_7_0_0.dtd">

<routes>
    <route>
        <pattern>/command/{commandName}/{structureId}</pattern>
        <generated-parameter name="mvcRenderCommandName">{commandName}</generated-parameter>
        <generated-parameter name="structureId">{structureId}</generated-parameter>
        <ignored-parameter name="p_auth"/>
        <implicit-parameter name="p_p_id">generatorviewclient</implicit-parameter>
        <implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
        <implicit-parameter name="p_p_state">normal</implicit-parameter>
        <implicit-parameter name="p_p_mode">view</implicit-parameter>
    </route>
    <route>
        <pattern>/resource/{resourceName}</pattern>
        <generated-parameter name="p_p_resource_id">{resourceName}</generated-parameter>
        <ignored-parameter name="p_auth"/>
        <implicit-parameter name="p_p_id">generatorviewclient</implicit-parameter>
        <implicit-parameter name="p_p_lifecycle">2</implicit-parameter>
        <implicit-parameter name="p_p_state">normal</implicit-parameter>
        <implicit-parameter name="p_p_mode">view</implicit-parameter>
    </route>
</routes>